{
  "name": "anahkiasen/autopergamene",
  "description": "Personal portfolio",
  "homepage": "http://autopergamene.eu",
  "license": "CC-BY-NC-ND-3.0",
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "4.2.*",
    "anahkiasen/flatten": "dev-develop",
    "anahkiasen/flickering": "dev-master",
    "anahkiasen/illuminage": "dev-master",
    "dflydev/markdown": "dev-master",
    "anahkiasen/rocketeer": "dev-develop",
    "anahkiasen/polyglot": "dev-develop",
    "knplabs/packagist-api": "dev-master",
    "anahkiasen/acetone": "dev-master",
    "anahkiasen/arrounded": "dev-master",
    "barryvdh/laravel-twigbridge": "dev-master",
    "twig/extensions": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "dev-master",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "Autopergamene": "app"
    },
    "classmap": [
      "app/controllers",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
