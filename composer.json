{
  "name": "anahkiasen/autopergamene",
  "description": "Personnal portfolio",
  "homepage": "http://autopergamene.eu",
  "license": "CC-BY-NC-ND-3.0",
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "4.1.*",
    "anahkiasen/cerberus": "dev-master",
    "anahkiasen/flickering": "dev-master",
    "anahkiasen/illuminage": "dev-master",
    "anahkiasen/underscore-php": "dev-master",
    "dflydev/markdown": "dev-master",
    "jasonlewis/basset": "dev-master",
    "natxet/CssMin": "dev-master",
    "opauth/flickr": "dev-master"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/database/migrations",
      "app/database/seeds",
      "app/models",
      "app/models/Repositories",
      "app/models/Services",
      "app/tests/TestCase.php"
    ]
  },
  "scripts": {
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-install-cmd": [
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
