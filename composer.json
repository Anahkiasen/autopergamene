{
  "name": "anahkiasen/autopergamene",
  "description": "Personnal portfolio",
  "homepage": "http://autopergamene.eu",
  "license": "CC-BY-NC-ND-3.0",
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "4.0.*",
    "anahkiasen/cerberus": "dev-master",
    "anahkiasen/glow": "dev-master",
    "anahkiasen/underscore-php": "dev-master",
    "cssmin/cssmin": "*",
    "dflydev/markdown": "dev-master",
    "imagine/Imagine": "dev-master",
    "jasonlewis/basset": "dev-master",
    "jasonlewis/expressive-date": "dev-master"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "cssmin/cssmin",
        "version": "3.0.1",
        "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
        "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
      }
    }
  ],
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/libraries",
      "app/models",
      "app/database/migrations",
      "app/tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev"
}